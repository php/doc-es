<refentry xml:id="xmldiff-file.merge" xmlns="http://docbook.org/ns/docbook" xmlns:xlink="http://www.w3.org/1999/xlink">
 <refnamediv>
  <refname>XMLDiff\File::merge</refname>
  <refpurpose>Produce un documento XML unido</refpurpose>
 </refnamediv>

 <refsect1 role="description">
  &reftitle.description;
  <methodsynopsis>
   <modifier>public</modifier> <type>string</type><methodname>XMLDiff\File::merge</methodname>
   <methodparam><type>string</type><parameter>src</parameter></methodparam>
   <methodparam><type>string</type><parameter>diff</parameter></methodparam>
  </methodsynopsis>
  <para>
   Crea un nuevo documento XML basado en diferencias y documento fuente.
  </para>
 </refsect1>

 <refsect1 role="parameters">
  &reftitle.parameters;
  <variablelist>
   <varlistentry>
    <term><parameter>src</parameter></term>
    <listitem>
     <para>
      Ruta al documento XML de origen.
     </para>
    </listitem>
   </varlistentry>
   <varlistentry>
    <term><parameter>diff</parameter></term>
    <listitem>
     <para>
      Ruta al documento XML con la informaci√≥n de diferencia.
     </para>
    </listitem>
   </varlistentry>
  </variablelist>
 </refsect1>

 <refsect1 role="returnvalues">
  &reftitle.returnvalues;
  <para>
   String con el nuevo documento XML o NULL.
  </para>
 </refsect1>


</refentry>

