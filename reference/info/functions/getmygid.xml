<refentry id="function.getmygid">
 <refnamediv>
  <refname>getmygid</refname>
  <refpurpose>Obtener el GID del due√±o del script PHP</refpurpose>
 </refnamediv>

 <refsect1 role="description">
  &reftitle.description;
  <methodsynopsis>
   <type>int</type><methodname>getmygid</methodname>
   <void/>
  </methodsynopsis>
  <simpara>
   Obtiene el ID de grupo del script actual.
  </simpara>
 </refsect1>

 <refsect1 role="returnvalues">
  &reftitle.returnvalues;
  <para>
   Devuelve el ID de grupo del script actual, o &false; en caso de error.
  </para>
 </refsect1>

