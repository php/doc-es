 <refnamediv>
  <refname>GearmanWorker::options</refname>
  <refpurpose>Obtiene las opciones del agente</refpurpose>
 </refnamediv>

 <refsect1 role="description">
  &reftitle.description;
  <methodsynopsis role="GearmanWorker">
   <modifier>public</modifier> <type>int</type><methodname>GearmanWorker::options</methodname>
   <void/>
  </methodsynopsis>
  <para>
   Obtiene las opciones previamente definidas para el agente.
  </para>
 </refsect1>

 <refsect1 role="parameters">
