    <constant>SVN_NODE_DIR</constant>
    (<type>int</type>)
   </term>
   <listitem>
    <simpara>
     Directorio
    </simpara>
   </listitem>
  </varlistentry>
  <varlistentry xml:id="constant.svn-node-unknown">
