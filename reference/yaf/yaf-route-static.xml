     <ooclass>
      <classname>Yaf_Route_Static</classname>
     </ooclass>
     
     <oointerface>
      <interfacename>Yaf_Router</interfacename>
     </oointerface>
    </classsynopsisinfo>
    <!-- }}} -->
    
    <classsynopsisinfo role="comment">&Methods;</classsynopsisinfo>
