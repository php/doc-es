 <variablelist xml:id="yaml.constants.break">
  <title>Tipos de saltos de línea para <function>yaml_emit</function></title>
  <varlistentry xml:id="constant.yaml-any-break">
   <term>
    <constant>YAML_ANY_BREAK</constant>
    (<type>int</type>)
   </term>
   <listitem>
    <simpara>
     Dejar que el emisor elija el carácter de salto de línea.
    </simpara>
   </listitem>
  </varlistentry>
  <varlistentry xml:id="constant.yaml-cr-break">
   <term>
    <constant>YAML_CR_BREAK</constant>
    (<type>int</type>)
   </term>
   <listitem>
    <simpara>
     Usar <literal>\r</literal> como carácter de salto (al estilo de Mac).
    </simpara>
   </listitem>
  </varlistentry>
  <varlistentry xml:id="constant.yaml-ln-break">
   <term>
    <constant>YAML_LN_BREAK</constant>
    (<type>int</type>)
   </term>
   <listitem>
    <simpara>
     Usar <literal>\n</literal> como carácter de salto (al estilo de Unix).
    </simpara>
   </listitem>
  </varlistentry>
  <varlistentry xml:id="constant.yaml-crln-break">
   <term>
    <constant>YAML_CRLN_BREAK</constant>
    (<type>int</type>)
   </term>
   <listitem>
    <simpara>
     Usar <literal>\r\n</literal> como cáracter de salto (al estilo de DOS).
    </simpara>
   </listitem>
  </varlistentry>
 </variablelist>

