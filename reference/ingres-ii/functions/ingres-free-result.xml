<refentry xml:id="function.ingres-free-result" xmlns="http://docbook.org/ns/docbook">
 <refnamediv>
  <refname>ingres_free_result</refname>
  <refpurpose>Libera los recursos asociados a un identificador de resultados</refpurpose>
 </refnamediv>
 
 <refsect1 role="description">
  &reftitle.description;
  <methodsynopsis>
   <type>bool</type><methodname>ingres_free_result</methodname>
   <methodparam><type>resource</type><parameter>result</parameter></methodparam>
  </methodsynopsis>
  <para>
  </para>
 </refsect1>
 
 <refsect1 role="parameters">
  &reftitle.parameters;
  <para>
   <variablelist>
    <varlistentry>
     <term><parameter>result</parameter></term>
     <listitem>
      <para>
       El identificador de resultados de la consulta
      </para>
     </listitem>
    </varlistentry>
   </variablelist>
  </para>
 </refsect1>
 <refsect1 role="returnvalues">
  &reftitle.returnvalues;
  <para>
   &return.success; 
  </para>
 </refsect1>
 
 <refsect1 role="examples">
  &reftitle.examples;
  <para>
   <example>
    <title>Liberar un recurso de resultados</title>
    <programlisting role="php">
<![CDATA[
<?php
$enlace = ingres_connect($base_datos, $usuario, $contraseÃ±a);

$resultado = ingres_query($enlace,"select * from table");

while ($fila = ingres_fetch_row($resultado)) {
    echo $fila[1];
    echo $fila[2];
}
ingres_free_result($resultado);

ingres_close($enlace)

?>
]]>
    </programlisting>
   </example>
  </para>
 </refsect1>
 
 <refsect1 role="seealso">
  &reftitle.seealso;
  <para>
   <simplelist>
    <member><function>ingres_query</function></member>
   </simplelist>
  </para>
 </refsect1>
 
</refentry>

