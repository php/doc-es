<!-- splitted from ./es/functions/uodbc.xml, last change in rev 1.1 -->
<!-- last change to 'odbc-rollback' in en/ tree in rev 1.2 -->
  <refentry id="function.odbc-rollback">
   <refnamediv>
    <refname>odbc_rollback</refname>
    <refpurpose>Volver a pasar una transacion</refpurpose>
   </refnamediv>
   <refsect1>
    <title>Descripcion</title>
     <methodsynopsis>
     <type>int</type><methodname>odbc_rollback</methodname>
     <methodparam><type>int</type><parameter>connection_id</parameter></methodparam>
     </methodsynopsis>
    <para>
     Vuelve a pasar todas las declaraciones pendientes 
     <parameter>connection_id</parameter>.  Devuelve 
     &true; cuando el resultado es satisfactorio, 
     &false; cuando no lo es.</para>
   </refsect1>
  </refentry>

