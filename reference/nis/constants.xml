<appendix xml:id="nis.constants" xmlns="http://docbook.org/ns/docbook">
 &reftitle.constants;
 &extension.constants;
 <variablelist>
  <varlistentry>
   <term>
    <constant>YPERR_ACCESS</constant> 
    (<type>integer</type>)
   </term>
   <listitem>
    <simpara>
     Violación de acceso (ha sido añadido recientemente y está sólo disponible paa PECL SVN por ahora)
    </simpara>
   </listitem>
  </varlistentry>
  <varlistentry>
   <term>
    <constant>YPERR_BADARGS</constant> 
    (<type>integer</type>)
   </term>
   <listitem>
    <simpara>
     Los argumentos de la función son incorrectos
    </simpara>
   </listitem>
  </varlistentry>
  <varlistentry>
   <term>
    <constant>YPERR_BADDB</constant> 
    (<type>integer</type>)
   </term>
   <listitem>
    <simpara>
     Base de datos YP es incorrecta
    </simpara>
   </listitem>
  </varlistentry>
  <varlistentry>
   <term>
    <constant>YPERR_BUSY</constant> 
    (<type>integer</type>)
   </term>
   <listitem>
    <simpara>
     Base de datos ocupada
    </simpara>
   </listitem>
  </varlistentry>
  <varlistentry>
   <term>
    <constant>YPERR_DOMAIN</constant> 
    (<type>integer</type>)
   </term>
   <listitem>
    <simpara>
     No puede enlazar al servidor sobre ese dominio
    </simpara>
   </listitem>
  </varlistentry>
  <varlistentry>
   <term>
    <constant>YPERR_KEY</constant> 
    (<type>integer</type>)
   </term>
   <listitem>
    <simpara>
     Clave no se encuentra en el mapa
    </simpara>
   </listitem>
  </varlistentry>
  <varlistentry>
   <term>
    <constant>YPERR_MAP</constant> 
    (<type>integer</type>)
   </term>
   <listitem>
    <simpara>
     Mapa no se encuentra en ese dominio
    </simpara>
   </listitem>
  </varlistentry>
  <varlistentry>
   <term>
    <constant>YPERR_NODOM</constant> 
    (<type>integer</type>)
   </term>
   <listitem>
    <simpara>
     Dominio local no establecido
    </simpara>
   </listitem>
  </varlistentry>
  <varlistentry>
   <term>
    <constant>YPERR_NOMORE</constant> 
    (<type>integer</type>)
   </term>
   <listitem>
    <simpara>
     No hay más registros en la base de datos
    </simpara>
   </listitem>
  </varlistentry>
  <varlistentry>
   <term>
    <constant>YPERR_PMAP</constant> 
    (<type>integer</type>)
   </term>
   <listitem>
    <simpara>
     No puede comunicarse con portmapper 
    </simpara>
   </listitem>
  </varlistentry>
  <varlistentry>
   <term>
    <constant>YPERR_RESRC</constant> 
    (<type>integer</type>)
   </term>
   <listitem>
    <simpara>
     Error en la asignación de recursos
    </simpara>
   </listitem>
  </varlistentry>
  <varlistentry>
   <term>
    <constant>YPERR_RPC</constant> 
    (<type>integer</type>)
   </term>
   <listitem>
    <simpara>
     Fallo RPC - dominio no ha sido contactado
    </simpara>
   </listitem>
  </varlistentry>
  <varlistentry>
   <term>
    <constant>YPERR_YPBIND</constant> 
    (<type>integer</type>)
   </term>
   <listitem>
    <simpara>
     No logra comunicarse con ypbind
    </simpara>
   </listitem>
  </varlistentry>
  <varlistentry>
   <term>
    <constant>YPERR_YPERR</constant> 
    (<type>integer</type>)
   </term>
   <listitem>
    <simpara>
     Error interno en servidor yp o error de cliente
    </simpara>
   </listitem>
  </varlistentry>
  <varlistentry>
   <term>
    <constant>YPERR_YPSERV</constant> 
    (<type>integer</type>)
   </term>
   <listitem>
    <simpara>
     No logra comunicarse con ypserv
    </simpara>
   </listitem>
  </varlistentry>
  <varlistentry>
   <term>
    <constant>YPERR_VERS</constant> 
    (<type>integer</type>)
   </term>
   <listitem>
    <simpara>
     La versión YP no coincide
    </simpara>
   </listitem>
  </varlistentry>
 </variablelist>
</appendix>

