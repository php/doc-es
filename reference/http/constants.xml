  <varlistentry xml:id="constant.http-params-raise-error">
   <term>
    <constant>HTTP_PARAMS_RAISE_ERROR</constant>
    (<type>integer</type>)
   </term>
   <listitem>
    <simpara>
     muestra avisos de PHP en errores de análisis
    </simpara>
   </listitem>
  </varlistentry>
