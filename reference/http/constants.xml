   <term>
    <constant>HTTP_PARAMS_DEFAULT</constant>
    (<type>integer</type>)
   </term>
   <listitem>
    <simpara>
     los tres valores superiores a nivel de bit
    </simpara>
   </listitem>
  </varlistentry>
 </variablelist>
