<refentry xml:id="function.printer-write" xmlns="http://docbook.org/ns/docbook">
 <refnamediv>
  <refname>printer_write</refname>
  <refpurpose>Escribir datos a la impresora</refpurpose>
 </refnamediv>
 
 <refsect1 role="description">
  &reftitle.description;
  <methodsynopsis>
   <type>bool</type><methodname>printer_write</methodname>
   <methodparam><type>resource</type><parameter>printer_handle</parameter></methodparam>
   <methodparam><type>string</type><parameter>content</parameter></methodparam>
  </methodsynopsis>
  <para>
   Escribe el par치metro <parameter>content</parameter> directamente a la impresora.
  </para>
 </refsect1>

 <refsect1 role="parameters">
  &reftitle.parameters;
  <para>
   <variablelist>
    <varlistentry>
     <term><parameter>printer_handle</parameter></term>
     <listitem>
      <para>
       El par치metro <parameter>printer_handle</parameter> debe ser un identificador v치lido para una impresora.
      </para>
     </listitem>
    </varlistentry>
    <varlistentry>
     <term><parameter>content</parameter></term>
     <listitem>
      <para>
       Los datos que se escribir치n.
      </para>
     </listitem>
    </varlistentry>
   </variablelist>
  </para>
 </refsect1>

 <refsect1 role="returnvalues">
  &reftitle.returnvalues;
  <para>
   &return.success;
  </para>
 </refsect1>

 <refsect1 role="examples">
  &reftitle.examples;
  <para>
   <example>
    <title>Ejemplo de <function>printer_write</function></title>
    <programlisting role="php">
<![CDATA[
<?php
$handle = printer_open();
printer_write($handle, "Texto a imprimir");
printer_close($handle);
?>
]]>
    </programlisting>
   </example>
  </para>
 </refsect1>
 
</refentry>

