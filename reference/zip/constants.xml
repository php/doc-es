  <varlistentry xml:id="ziparchive.constants.fl-nodir">
   <term>
    <constant>ZipArchive::FL_NODIR</constant>
    (<type>int</type>)
   </term>
   <listitem>
    <simpara>
     Ignorar el componente directorio
