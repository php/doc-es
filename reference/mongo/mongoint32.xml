<phpdoc:classref xml:id="class.mongoint32" xmlns:phpdoc="http://php.net/ns/phpdoc" xmlns="http://docbook.org/ns/docbook" xmlns:xlink="http://www.w3.org/1999/xlink" xmlns:xi="http://www.w3.org/2001/XInclude">
 
 <title>Clase MongoInt32</title>
 <titleabbrev>MongoInt32</titleabbrev>
 
 <partintro>
  
  <!-- {{{ MongoInt32 intro -->
  <section xml:id="mongoint32.intro">
   &reftitle.intro;
   <para>
    Esta clase se puede usar para guardar enteros de 32 bits en bases de datos de
    sistemas a 64 bits.
   </para>
  </section>
  <!-- }}} -->
  
  <section xml:id="mongoint32.synopsis">
   &reftitle.classsynopsis;
   
   <!-- {{{ Synopsis -->
   <classsynopsis>
    <ooclass><classname>MongoInt32</classname></ooclass>
    
    <!-- {{{ Class synopsis -->
    <classsynopsisinfo>
     <ooclass>
      <classname>MongoInt32</classname>
     </ooclass>
    </classsynopsisinfo>
    <!-- }}} -->
    
    <classsynopsisinfo role="comment">Campos</classsynopsisinfo>
    <fieldsynopsis>
     <modifier>public</modifier>
     <type>string</type>
     <varname linkend="mongoint32.props.value">value</varname>
    </fieldsynopsis>
    
    <classsynopsisinfo role="comment">&Methods;</classsynopsisinfo>
    <xi:include xpointer="xmlns(db=http://docbook.org/ns/docbook) xpointer(id('class.mongoint32')/db:refentry/db:refsect1[@role='description']/descendant::db:methodsynopsis[1])" />
   </classsynopsis>
   <!-- }}} -->
   
  </section>
  
  <section>
   <title>Campos</title>
   <variablelist>
    <varlistentry xml:id="mongoint32.props.value">
     <term><varname>value</varname></term>
     <listitem>
      <simpara>
       Este es el valor en forma de texto del número de 32 bits. Por ejemplo, el valor de 123
       sería "123".
      </simpara>
     </listitem>
    </varlistentry>
   </variablelist>
  </section>
  
 </partintro>
 
 &reference.mongo.entities.mongoint32;
 
</phpdoc:classref>

