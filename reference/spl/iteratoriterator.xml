     <ooclass>
      <classname>IteratorIterator</classname>
     </ooclass>

     <oointerface>
      <interfacename>Iterator</interfacename>
     </oointerface>

     <oointerface>
      <interfacename>Traversable</interfacename>
     </oointerface>

     <oointerface>
