<refentry xml:id="filesystemiterator.getflags" xmlns="http://docbook.org/ns/docbook" xmlns:xlink="http://www.w3.org/1999/xlink">
 <refnamediv>
  <refname>FilesystemIterator::getFlags</refname>
  <refpurpose>Lee las opciones activadas</refpurpose>
 </refnamediv>

 <refsect1 role="description">
  &reftitle.description;
  <methodsynopsis role="FilesystemIterator">
   <modifier>public</modifier> <type>int</type><methodname>FilesystemIterator::getFlags</methodname>
   <void/>
  </methodsynopsis>
  <para>
   Lee las opciones activadas, tal como fueron
   configuradas con <methodname>FilesystemIterator::__construct</methodname>
   o <methodname>FilesystemIterator::setFlags</methodname>.
  </para>
 </refsect1>

 <refsect1 role="parameters">
  &reftitle.parameters;
  &no.function.parameters;
 </refsect1>

 <refsect1 role="returnvalues">
  &reftitle.returnvalues;
  <para>
   El valor de las opciones configuradas.
  </para>
 </refsect1>

 <refsect1 role="seealso">
  &reftitle.seealso;
  <para>
   <simplelist>
    <member><methodname>FilesystemIterator::__construct</methodname></member>
    <member><methodname>FilesystemIterator::setFlags</methodname></member>
   </simplelist>
  </para>
 </refsect1>

</refentry>

