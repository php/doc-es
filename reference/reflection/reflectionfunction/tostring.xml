<refentry xml:id="reflectionfunction.tostring" xmlns="http://docbook.org/ns/docbook" xmlns:xlink="http://www.w3.org/1999/xlink">
 <refnamediv>
  <refname>ReflectionFunction::__toString</refname>
  <refpurpose>Convertir a string</refpurpose>
 </refnamediv>

 <refsect1 role="description">
  &reftitle.description;
  <methodsynopsis>
   <modifier>public</modifier> <type>string</type><methodname>ReflectionFunction::__toString</methodname>
   <void />
  </methodsynopsis>
  <para>
   Convierte a string.
  </para>
 </refsect1>

 <refsect1 role="parameters">
  &reftitle.parameters;
  &no.function.parameters;
 </refsect1>

 <refsect1 role="returnvalues">
  &reftitle.returnvalues;
  <para>
   Devuelve una salida similar a la de
   <methodname>ReflectionFunction::export</methodname>.
  </para>
 </refsect1>

 <refsect1 role="examples">
  &reftitle.examples;
  <para>
   <example>
    <title>Ejemplo de <methodname>ReflectionFunction::__toString</methodname></title>
    <programlisting role="php">
<![CDATA[
<?php
function titulo($titulo, $nombre)
{
    return sprintf("%s. %s\r\n", $titulo, $nombre);
}

echo new ReflectionFunction('titulo');
?>
]]>
    </programlisting>
    &example.outputs.similar;
    <screen>
<![CDATA[
Function [ <user> function titulo ] {
  @@ Command line code 2 - 5

  - Parameters [2] {
    Parameter #0 [ <required> $titulo ]
    Parameter #1 [ <required> $nombre ]
  }
}
]]>
    </screen>
   </example>
  </para>
 </refsect1>

 <refsect1 role="seealso">
  &reftitle.seealso;
  <para>
   <simplelist>
    <member><methodname>ReflectionFunction::export</methodname></member>
    <member><methodname>ReflectionClass::clone</methodname></member>
    <member><link linkend="language.oop5.magic.tostring">__toString</link></member>
   </simplelist>
  </para>
 </refsect1>

</refentry>

