<appendix xml:id="net-gopher.constants" xmlns="http://docbook.org/ns/docbook">
 &reftitle.constants;
 &extension.constants;
 <table>
  <title>Constantes de Net Gopher</title>
  <tgroup cols="3">
   <thead>
    <row>
     <entry>Constante</entry>
     <entry>Valor</entry>
     <entry>Descripción</entry>
    </row>
   </thead>
   <tbody>
    <row>
     <entry><constant>GOPHER_DOCUMENT</constant></entry>
     <entry>0</entry>
     <entry>Documento <literal>text/plain</literal> estándar.</entry>
    </row>
    <row>
     <entry><constant>GOPHER_DIRECTORY</constant></entry>
     <entry>1</entry>
     <entry>Un recurso que contiene una lista de directorios con formato gopher.</entry>
    </row>
    <row>
     <entry><constant>GOPHER_BINHEX</constant></entry>
     <entry>4</entry>
     <entry>Un fichero binario codificado con BinHex.</entry>
    </row>
    <row>
     <entry><constant>GOPHER_DOSBINARY</constant></entry>
     <entry>5</entry>
     <entry>Un fichero binario con formato DOS.</entry>
    </row>
    <row>
     <entry><constant>GOPHER_UUENCODED</constant></entry>
     <entry>6</entry>
     <entry>Un fichero UUEncoded.</entry>
    </row>
    <row>
     <entry><constant>GOPHER_BINARY</constant></entry>
     <entry>9</entry>
     <entry>Un fichero binario genérico.</entry>
    </row>
    <row>
     <entry><constant>GOPHER_INFO</constant></entry>
     <entry>255</entry>
     <entry>Una entrada informativa</entry>
    </row>
    <row>
     <entry><constant>GOPHER_HTTP</constant></entry>
     <entry>254</entry>
     <entry>Una referencia a un recurso HTTP.</entry>
    </row>
    <row>
     <entry><constant>GOPHER_UNKNOWN</constant></entry>
     <entry>-1</entry>
     <entry>
      Una entrada no reconocida.
     </entry>
    </row>
   </tbody>
  </tgroup>
 </table>
</appendix>

