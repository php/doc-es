<refentry xml:id='function.domattribute-name' xmlns="http://docbook.org/ns/docbook">
 <refnamediv>
  <refname>DomAttribute->name</refname>
  <refpurpose>
   Devuelve el nombre de un atributo
  </refpurpose>
 </refnamediv>
 <refsect1 role="description">
  &reftitle.description;
  <classsynopsis>
   <ooclass><classname>DomAttribute</classname></ooclass>
   <methodsynopsis>
    <type>string</type><methodname>name</methodname>
    <void/>
   </methodsynopsis>
  </classsynopsis>
  <para>
   Obtiene el nombre del atributo.
  </para>
 </refsect1>
 <refsect1 role="returnvalues">
  &reftitle.returnvalues;
  <para>
   Devuelve el nombre del atributo.
  </para>
 </refsect1>
 <refsect1 role="migration">
  <title>Migraci&oacute;n a PHP 5</title>
  <para>
   Use la propiedad <property>name</property> de
   <classname>DOMAttr</classname>.
  </para>
 </refsect1>
 <refsect1 role="seealso">
  &reftitle.seealso;
  <para>
   <simplelist>
    <member><xref linkend="function.domattribute-value" /> para un
    ejemplo</member>
   </simplelist>
  </para>
 </refsect1>
</refentry>

