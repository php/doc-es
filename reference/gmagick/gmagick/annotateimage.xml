<refentry xmlns="http://docbook.org/ns/docbook" xmlns:xlink="http://www.w3.org/1999/xlink" xml:id="gmagick.annotateimage">
 <refnamediv>
  <refname>Gmagick::annotateimage</refname>
  <refpurpose>Anota una imagen con texto</refpurpose>
 </refnamediv>

 <refsect1 role="description">
  &reftitle.description;
  <methodsynopsis>
   <modifier>public</modifier> <type>Gmagick</type><methodname>Gmagick::annotateimage</methodname>
   <methodparam><type>GmagickDraw</type><parameter>GmagickDraw</parameter></methodparam>
   <methodparam><type>float</type><parameter>x</parameter></methodparam>
   <methodparam><type>float</type><parameter>y</parameter></methodparam>
   <methodparam><type>float</type><parameter>angle</parameter></methodparam>
   <methodparam><type>string</type><parameter>text</parameter></methodparam>
  </methodsynopsis>
  <simpara>
   Anota una imagen con texto.
  </simpara>
 </refsect1>

 <refsect1 role="parameters">
  &reftitle.parameters;
  <variablelist>
   <varlistentry>
    <term><parameter>GmagickDraw</parameter></term>
    <listitem>
     <simpara>
      El objeto <classname>GmagickDraw</classname> que contiene la configuración para dibujar el texto
     </simpara>
    </listitem>
   </varlistentry>
   <varlistentry>
    <term><parameter>x</parameter></term>
    <listitem>
     <simpara>
      El índice horizontal en píxeles a la izquierda del texto.
     </simpara>
    </listitem>
   </varlistentry>
   <varlistentry>
    <term><parameter>y</parameter></term>
    <listitem>
     <simpara>
      El índice vertical en píxeles a la línea base del texto.
     </simpara>
    </listitem>
   </varlistentry>
   <varlistentry>
    <term><parameter>angle</parameter></term>
    <listitem>
     <simpara>
      El ángulo en el que escribir el texto.
     </simpara>
    </listitem>
   </varlistentry>
   <varlistentry>
    <term><parameter>text</parameter></term>
    <listitem>
     <simpara>
      La cadena a dibujar.
     </simpara>
    </listitem>
   </varlistentry>
  </variablelist>
 </refsect1>

 <refsect1 role="returnvalues">
  &reftitle.returnvalues;
  <simpara>
   El objeto <classname>Gmagick</classname> con la anotación hecha.
  </simpara>
 </refsect1>

 <refsect1 role="errors">
  &reftitle.errors;
  <simpara>
   &gmagick.gmagickexception.throw;
  </simpara>
 </refsect1>
</refentry>
