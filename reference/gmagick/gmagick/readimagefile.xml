<refentry xml:id="gmagick.readimagefile" xmlns="http://docbook.org/ns/docbook" xmlns:xlink="http://www.w3.org/1999/xlink">
 <refnamediv>
  <refname>Gmagick::readimagefile</refname>
  <refpurpose>Lee una imagen desde un descriptor de archivo abierto</refpurpose>
 </refnamediv>

 <refsect1 role="description">
  &reftitle.description;
  <methodsynopsis>
   <modifier>public</modifier> <type>Gmagick</type><methodname>Gmagick::readimagefile</methodname>
   <methodparam><type>string</type><parameter>fp</parameter></methodparam>
  </methodsynopsis>
  <para>
   Lee una imagen o secuencia de imágenes desde un descriptor de archivo abierto.
  </para>

   

 </refsect1>

 <refsect1 role="parameters">
  &reftitle.parameters;
  <para>
   <variablelist>
    <varlistentry>
     <term><parameter>fp</parameter></term>
     <listitem>
      <para>
       El descriptor de archivo.
      </para>
     </listitem>
    </varlistentry>
   </variablelist>
  </para>
 </refsect1>

 <refsect1 role="returnvalues">
  &reftitle.returnvalues;
  <para>
   El objeto Gmagick si se tuvo éxito
  </para>
 </refsect1>

 <refsect1 role="errors">
  &reftitle.errors;
  <para>
   &gmagick.gmagickexception.throw;
  </para>
 </refsect1>

</refentry>

