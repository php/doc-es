<refentry xmlns="http://docbook.org/ns/docbook" xmlns:xlink="http://www.w3.org/1999/xlink" xml:id="gmagick.writeimage">
 <refnamediv>
  <refname>Gmagick::writeimage</refname>
  <refpurpose>Escribe una imagen en un archivo</refpurpose>
 </refnamediv>

 <refsect1 role="description">
  &reftitle.description;
  <methodsynopsis>
   <modifier>public</modifier> <type>Gmagick</type><methodname>Gmagick::writeimage</methodname>
   <methodparam><type>string</type><parameter>filename</parameter></methodparam>
   <methodparam choice="opt"><type>bool</type><parameter>all_frames</parameter><initializer>&false;</initializer></methodparam>
  </methodsynopsis>
  <simpara>
   Escribe una imagen en un archivo específico. Si el argumento
   <parameter>filename</parameter> es &null;, la imagen
   será escrita en el archivo definido por el método <methodname>Gmagick::readimage</methodname>
   o el método <methodname>Gmagick::setimagefilename</methodname>.
  </simpara>
 </refsect1>

 <refsect1 role="parameters">
  &reftitle.parameters;
  <variablelist>
   <varlistentry>
    <term><parameter>filename</parameter></term>
    <listitem>
     <simpara>
      El nombre del archivo.
     </simpara>
    </listitem>
   </varlistentry>
  </variablelist>
 </refsect1>

 <refsect1 role="returnvalues">
  &reftitle.returnvalues;
  <simpara>
   El objeto <classname>Gmagick</classname>.
  </simpara>
 </refsect1>

 <refsect1 role="errors">
  &reftitle.errors;
  <simpara>
   &gmagick.gmagickexception.throw;
  </simpara>
 </refsect1>
</refentry>
